<Application
    x:Class="FactoryMonitor.Client.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:FactoryMonitor.Client">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/FactoryMonitor.UserControls;component/Resources/XAML/CommonColors.xaml" />
                <ResourceDictionary Source="/FactoryMonitor.UserControls;component/Resources/XAML/ControlBarStyle.xaml" />

                <ResourceDictionary>
                    <FontFamily x:Key="IconFont">
                        FontFamily="pack://application:,,,/FactoryMonitor.UserControls;component/Resources/Font/#iconfont"
                    </FontFamily>
                </ResourceDictionary>

            </ResourceDictionary.MergedDictionaries>



        </ResourceDictionary>
    </Application.Resources>
</Application>
