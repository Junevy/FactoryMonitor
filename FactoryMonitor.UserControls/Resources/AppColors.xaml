<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BorderForeground" Color="#d3d3d3" />
    <SolidColorBrush x:Key="PressedBackground" Color="#dcdcdc" />
    <SolidColorBrush x:Key="WarnBackground" Color="#ff6347" />
    <SolidColorBrush x:Key="WarnForeground" Color="Black" />
    <SolidColorBrush x:Key="SelectedBackground" Color="#dcdcdc" />
    <SolidColorBrush x:Key="SelectedForeground" Color="BlueViolet" />

    <!--  sub color  -->
    <SolidColorBrush x:Key="SubBackground" Color="#f5f5f5" />
    <SolidColorBrush x:Key="SubTextForeground" Color="#696969" />

</ResourceDictionary>